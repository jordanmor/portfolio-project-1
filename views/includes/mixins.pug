mixin article(title, projectCategory, urlParams)
  article.grid-container.portfolio-intro
    div
      h2.title=title
  
  article.grid-container.portfolio-index
    .grid-x.grid-margin-x.small-up-2.medium-up-2.large-up-3

      each project in projects[projectCategory]
        - const { id, project_name, image_urls } = project; 
        .cell
          a(href=`/projects/${urlParams}/${id}`)
            img.thumbnail(src=`${image_urls[0]}`)
            h5= project_name